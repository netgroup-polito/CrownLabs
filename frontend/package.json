    "@ant-design/icons": "^5.6.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@apollo/client": "^3.13.8",
    "@tailwindcss/vite": "^4.1.5",
    "antd": "^5.25.0",
    "clsx": "^2.1.1",
    "csstype": "^3.1.3",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "oidc-client-ts": "^3.2.1",
