{
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "webpack -p --config webpack_docker.config.js --display-error-details",
    "prettify-all": "node_modules/.bin/prettier --write **/*.{js,json,css}",
    "lint-check": "node_modules/.bin/prettier --check **/*.{js,json,css}",
    "lint-setup": " npm i -D prettier eslint eslint-config-airbnb eslint-config-prettier eslint-plugin-import eslint-plugin-jsx-a11y eslint-plugin-prettier eslint-plugin-react eslint-plugin-react-hooks"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "2.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@kubernetes/client-node": "file:./k8s_library/",
    "oidc-client": "^1.10.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "react-bootstrap": "^1.0.0-beta.17",
    "robotstxt-webpack-plugin": "^7.0.0",
    "bootstrap": "^4.4.1",
    "webpack": "^4.42.0",
    "html-webpack-plugin": "^3.2.0",
    "toastr": "^2.1.4"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {}
  }
}
